{
  "name": "vicente-peris/php-mrw-api",
  "type": "project",
  "description": "Api Client to connect to MRW Services",
  "minimum-stability": "stable",
  "keywords": [
    "Api",
    "Client",
    "MRW"
  ],
  "homepage": "https://github.com/vicente-peris/php-mrw-api",
  "license": "MIT",
  "authors": [
    {
      "name": "MICROVALENCIA - Vicente Peris",
      "email": "vicente@microvalencia.es"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "scripts": {
    "test": "php phars/phpunit.phar --color=always",
    "test-coverage": "php phars/phpunit.phar --coverage-html coverage",
    "test-coverage-cli": "php phars/phpunit.phar --coverage-text"
  }
}
